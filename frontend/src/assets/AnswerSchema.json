{
  "$schema": "http://json-schema.org/draft-07/schema",
  "version": "0.0.1",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "uid": {
      "title": "ユーザID",
      "description": "ユーザID",
      "type": ["integer", "string"],
      "pattern": "^[a-zA-Z0-9_-]+$",
      "examples": [
        "user_a"
      ]
    },
    "qid": {
      "title": "問題ID",
      "description": "問題ID",
      "type": ["integer", "string"],
      "pattern": "^[a-zA-Z0-9_-]+$",
      "examples": [
        1
      ]
    },
    "isCorrect": {
      "title": "正答フラグ",
      "description": "正答フラグ",
      "type": "boolean",
      "examples": [
        true
      ]
    }
  }
}
